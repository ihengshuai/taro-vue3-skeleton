{
  "typescript.tsdk": "node_modules/typescript/lib",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "$(capture).test.ts, $(capture).test.tsx",
    "*.tsx": "$(capture).test.ts, $(capture).test.tsx",
    "*.env": "$(capture).env.*",
    "Dockerfile": ".dockerfileignore",
    "package.json": "CHANGELOG*,yarn.lock,pnpm-lock.*,LICENSE,.gitattributes,.gitignore,.gitlab-ci.yml,.npmrc,.browserslistrc,.cz-config.js",
    ".eslintrc": ".eslintignore,.prettierignore,babel.config.js,.stylelintignore,.commitlintrc.js,.prettierrc.cjs,.stylelintrc.js,.editorconfig"
  },
}
